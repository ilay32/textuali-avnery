Options -Indexes +SymLinksIfOwnerMatch
RewriteEngine on
RewriteBase /
RewriteCond %{REQUEST_URI} ^/$
RewriteRule (.*)$ {{lang}}/ [L] 
RewriteCond %{REQUEST_URI} ^/[a-z0-9\-_]*.html$
RewriteRule (.*) {{lang}}/$1 [L] 
